# xml declaration
snippet xml
	<?xml version="1.0" encoding="UTF-8"?>

snippet tag
	<${1:}>
		${2}
	</$1>
# inline tag
snippet tas
	<${1:}>${2}</$1>

snippet it1
  <${1:#:name} ${2:#:item}="${3:#:value}"/>

snippet it2
  <${1:#:name} ${2:#:item}="${3:#:value}" ${4:#:item}="${5:#:value}"/>

snippet it3
  <${1:#:name} ${2:#:item}="${3:#:value}" ${4:#:item}="${5:#:value}" ${6:#:item}="${7:#:value}"/>
